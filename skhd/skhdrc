alt - j : yabai -m window --focus south
alt - k : yabai -m window --focus north
alt - h : yabai -m window --focus west
alt - l : yabai -m window --focus east

ctrl - t : yabai -m space --create
ctrl - w : yabai -m space --destroy

ctrl - 1 : yabai -m space --focus 1
ctrl - 2 : yabai -m space --focus 2
ctrl - 3 : yabai -m space --focus 3
ctrl - 4 : yabai -m space --focus 4
ctrl - 5 : yabai -m space --focus 5
ctrl - 6 : yabai -m space --focus 6
ctrl - 7 : yabai -m space --focus 7
ctrl - 8 : yabai -m space --focus 8
ctrl - 9 : yabai -m space --focus last

ctrl - up : yabai -m space --focus recent
ctrl - left : yabai -m space --focus prev
ctrl - right : yabai -m space --focus next

shift + ctrl - left : yabai -m space --move prev
shift + ctrl - right: yabai -m space --move next

shift + alt - t : yabai -m window --toggle float --grid 4:4:1:1:2:2

# alt + cmd - space : osascript -e 'tell application "iTerm" to create window with default profile'

alt + cmd - space : open -n /Applications/Ghostty.app
